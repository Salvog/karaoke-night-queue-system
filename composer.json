{
    "name": "karaoke-night/queue-system",
    "description": "Karaoke Night Queue System",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "laravel/pint": "^1.13",
        "phpstan/phpstan": "^1.11",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "test": "php artisan test",
        "stan": "phpstan analyse",
        "pint": "pint"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
